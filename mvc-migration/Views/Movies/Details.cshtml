@model mvc_migration.Models.Movies
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.Genre.Name</li>
    <li>ReleaseDate: @Model.ReleaseDate.ToShortDateString()</li>
    <li>DateAdded: @Model.DateAdded.ToShortDateString()</li>
    <li>NumberInStocks: @Model.NumberInStock</li>
</ul>

